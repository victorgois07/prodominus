{
  "name": "prodominus",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "validate-env": "node scripts/validate-env.js",
    "dev": "npm run validate-env && concurrently \"next dev --turbopack\" \"json-server --watch db.json --port 3001\"",
    "build": "npm run validate-env && next build",
    "start": "npm run validate-env && concurrently \"next start\" \"json-server --watch db.json --port 3001\"",
    "lint": "next lint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:snapshot": "vitest run --update-snapshot",
    "server": "json-server --watch db.json --port 3001"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@tanstack/react-query": "^5.76.1",
    "next": "15.3.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.56.3",
    "react-number-format": "^5.4.4",
    "reflect-metadata": "^0.2.2",
    "tailwind-merge": "^3.3.0",
    "tsyringe": "^4.10.0",
    "zustand": "^5.0.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^3.1.3",
    "@vitest/ui": "^3.1.3",
    "concurrently": "^8.2.2",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "jsdom": "^26.1.0",
    "json-server": "^0.17.4",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^3.1.3"
  }
}
